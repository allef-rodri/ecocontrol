{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["idEquipamento", "deEquipamento", "tipo", "consumoMaximo", "status", "setor"],
  "properties": {
    "idEquipamento": { "type": "number" },
    "deEquipamento": { "type": "string", "minLength": 1 },
    "tipo": { "type": "string", "minLength": 1 },
    "consumoMaximo": { "type": "number" },
    "status": { "type": "string", "minLength": 1 },
    "setor": {
      "type": "object",
      "required": ["idSetor", "deSetor", "localizacao"],
      "properties": {
        "idSetor": { "type": "number" },
        "deSetor": { "type": "string", "minLength": 1 },
        "localizacao": { "type": "string", "minLength": 1 }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
